    ( exists dbNSFP_SIFT_pred &  dbNSFP_SIFT_pred =~ 'D')|
    ( exists dbNSFP_Polyphen2_HVAR_pred & dbNSFP_Polyphen2_HVAR_pred =~ 'D' )|
